<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="presenter"
            type="com.fcn.park.property.contract.PropertyPlantLeaseContract.Presenter" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/include"
            layout="@layout/app_layout_title" />

        <com.liaoinstan.springview.widget.SpringView
            android:id="@+id/spring_view_plant_lease_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.fcn.park.base.widget.EmptyRecyclerView
                android:id="@+id/property_plant_lease"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </com.liaoinstan.springview.widget.SpringView>

        <include
            android:id="@+id/empty_plant_lease_list"
            layout="@layout/include_empty_layout" />

    </LinearLayout>
</layout>