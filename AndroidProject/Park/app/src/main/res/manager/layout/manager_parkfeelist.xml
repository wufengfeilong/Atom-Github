<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/app_layout_title" />
        <com.liaoinstan.springview.widget.SpringView
            android:id="@+id/spring_view_manager_parkfeelist"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.fcn.park.base.widget.EmptyRecyclerView
                android:id="@+id/recycler_view_manager_parkfeelist"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/windowBackground" />

        </com.liaoinstan.springview.widget.SpringView>

        <include
            android:id="@+id/parking_fee_list_empty_view"
            layout="@layout/include_empty_layout" />
    </LinearLayout>
</layout>
